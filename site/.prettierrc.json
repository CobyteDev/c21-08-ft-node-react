    "trailingComma": "es5",
    "tabWidth": 2,
    "semi": false,
    "singleQuote": true,
    "jsxSingleQuote": true,
    "arrowParens": "avoid",
    "bracketSpacing": true,
    "bracketSameLine": false,
    "singleAttributePerLine": false,
    "plugins": ["prettier-plugin-tailwindcss"]
}